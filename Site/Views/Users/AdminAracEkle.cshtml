@model Site.Models.AracKayit

@{
    ViewData["Title"] = "AdminAracEkle";
    var vm = new UserDetailsViewModel();
    var users = vm.Araclar;
}

<form asp-controller="Users" asp-action="AdminAracEkle" method="post">
    <div class="form-group">
        <label class="control-label col-md-2" for="UserId">Kullanıcılar</label>
        <div class="col-md-10">
            @Html.DropDownList("UserId", String.Empty)
            @Html.ValidationMessageFor(model => model.User.DaireNo)
        </div>
    </div>
    <div class="form-group">
        <label asp-for="Plaka">Plaka</label>
        <input asp-for="Plaka" class="form-control" />
        <span asp-validation-for="Plaka" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Marka">Marka</label>
        <input asp-for="Marka" class="form-control" />
        <span asp-validation-for="Marka" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Kaydet</button>
</form>
